GridBackground {
    --grid-background-color: rgb(22, 23, 25);
    --line-color: rgb(13, 15, 16);
    --thick-line-color: rgb(5, 6, 6);
    --spacing: 25;
    --line-count: 1;
    -unity-font-style: normal;
}

.text-field {
    width: 899px;
    white-space: nowrap;
    flex-basis: auto;
}

.text-field > .unity-base-field__input {
    background-color: rgba(0, 0, 0, 0);
    border-color: rgba(0, 0, 0, 0);
    border-color: rgba(0, 0, 0, 0);
    -unity-text-align: middle-center;
}
